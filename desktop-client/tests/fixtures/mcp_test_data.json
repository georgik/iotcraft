  "expected_command_translations": {
    "list_devices": "list",
    "control_device_on": "control kitchen_lamp_01 ON",
    "control_device_off": "control kitchen_lamp_01 OFF",
    "spawn_lamp": "spawn kitchen_lamp_01 3.5 1 2",
    "spawn_door": "spawn_door main_door 0 1 5",
    "place_block": "place grass 10 1 5",
    "place_block_float": "place dirt 5 2 -1",
    "remove_block": "remove 10 1 5",
    "create_wall": "wall stone 0 0 0 3 2 1",
    "move_device": "move kitchen_lamp_01 5 1 3",
    "teleport_camera": "tp 10 20 -5",
    "set_camera_angle": "look 45 -30",
    "save_world": "save_map test_world.json",
    "load_world": "load_map test_world.json"
  },
  "error_cases": {
    "missing_required_params": [
      {
        "tool": "create_wall",
        "args": {"block_type": "stone", "x1": 0},
        "expected_error": "x2 parameter is required"
      },
      {
        "tool": "place_block", 
        "args": {"block_type": "grass"},
        "expected_error": "x parameter is required"
      },
      {
        "tool": "spawn_device",
        "args": {"device_id": "test"},
        "expected_error": "device_type parameter is required"
      }
    ],
    "invalid_parameter_types": [
      {
        "tool": "place_block",
        "args": {"block_type": "grass", "x": "not_a_number", "y": 1, "z": 1},
        "expected_result": null
      },
      {
        "tool": "create_wall",
        "args": {"block_type": "stone", "x1": "invalid", "y1": 0, "z1": 0, "x2": 1, "y2": 1, "z2": 1},
        "expected_result": null
      }
    ],
    "invalid_device_types": [
      {
        "tool": "spawn_device",
        "args": {"device_id": "test", "device_type": "invalid_type", "x": 1, "y": 1, "z": 1},
        "expected_result": null
      }
    ]
  },
  "stress_test_cases": {
    "large_wall": {
      "tool": "create_wall",
      "args": {
        "block_type": "stone",
        "x1": -50,
        "y1": -10,
        "z1": -25,
        "x2": 50,
        "y2": 10,
        "z2": 25
      },
      "expected_blocks": 101 * 21 * 51
    },
    "negative_coordinates": {
      "tool": "place_block",
      "args": {
        "block_type": "dirt",
        "x": -1000,
        "y": -500,
        "z": -2000
      }
    },
    "extreme_device_positions": {
